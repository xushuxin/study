 * 在index.js基础上自己写
 * num1,num2必须是字符串
 */
function multiply(num1,num2){
  let arr1=num1.split('').reverse();
  let arr2=num2.split('').reverse();
  let preFirst=0;//上次相乘的结果十位
  let first=0;//十位
  let second=0;//个位
  let resArr=[];
  arr1.forEach((item1,index1)=>{
    arr2.forEach((item2,index2)=>{//index1 index2 0 0 ,0 1, 0 2 ; 1 0, 1 1, 1 2; 2 0,2 1, 2,2 
      let arr = (item1*item2).toString().split('');
      if(arr.length===1) arr.unshift('0')
      console.log(arr)
      let k=0;
      while(k<arr.length){
        resArr[index1+index2]=arr[1]
      }
      
    })
  })
  // console.log(arr1)
  // console.log(arr2)
  console.log(resArr)
}
multiply('123','456')
  // 123
  // 456
